.swipe-cell {
  position: relative;
  overflow: hidden;
}
.swipe-cell-left,
.swipe-cell-right {
  position: absolute;
  top: 0;
  height: 100%;
}
.swipe-cell-left {
  left: 0;
  -webkit-transform: translate3d(-100%, 0, 0);
  transform: translate3d(-100%, 0, 0);
}
.swipe-cell-right {
  right: 0;
  -webkit-transform: translate3d(100%, 0, 0);
  transform: translate3d(100%, 0, 0);
}
