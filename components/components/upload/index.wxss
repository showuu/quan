.container {
  display: flex;
  flex-flow: row wrap;
}
.cell {
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
}
.upload_area {
  box-sizing: border-box;
  width: 200rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border: 1px dashed #e5e5e4;
  font-size: 24rpx;
  color: #acacac;
}
.upload_icon {
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 10rpx;
}
.border_input {
  border: 1px solid #e5e5e4;
  border-radius: 10rpx;
}
.upload_file_wrapper {
  box-sizing: border-box;
  width: 200rpx;
  height: 200rpx;
  position: relative;
  margin-right: 20rpx;
}
.upload_file {
  box-sizing: border-box;
  width: 200rpx;
  height: 200rpx;
  border: 1px solid #f8f8f8;
}
.op_area {
  box-sizing: border-box;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  padding: 10rpx 0;
  display: flex;
  justify-content: space-evenly;
  background-color: rgba(26, 26, 26, 0.5);
}
.op_icon {
  width: 36rpx;
}
